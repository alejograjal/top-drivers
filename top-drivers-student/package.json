{
  "name": "top-drivers-student",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "top-drivers:generate": "openapi-typescript ./src/api/topdrivers/spec.json -o ./src/api/topdrivers/api.d.ts",
    "top-drivers:update": "curl -s https://escuelademanejotopdrivers.com/api/swagger/v1/swagger.json -o ./src/api/topdrivers/spec.json && sed -i '' 's/; x-api-version=[0-9]*//' ./src/api/topdrivers/spec.json && npx openapi-typescript ./src/api/topdrivers/spec.json -o ./src/api/topdrivers/api.d.ts",
    "top-drivers:update-local": "curl -s http://localhost:5191/swagger/v1/swagger.json -o ./src/api/topdrivers/spec.json && sed -i '' 's/; x-api-version=[0-9]*//' ./src/api/topdrivers/spec.json && npx openapi-typescript ./src/api/topdrivers/spec.json -o ./src/api/topdrivers/api.d.ts"
  },
  "dependencies": {
    "@emotion/react": "^11.13.3",
    "@emotion/styled": "^11.13.0",
    "@mui/icons-material": "^6.1.6",
    "@mui/material": "^6.1.5",
    "@tanstack/react-query": "^5.59.16",
    "openapi-fetch": "^0.13.0",
    "openapi-typescript": "^6.3.7",
    "openapi-typescript-fetch": "^2.0.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-helmet": "^6.1.0",
    "react-helmet-async": "^2.0.5",
    "react-image-gallery": "^1.3.0",
    "react-router-dom": "^6.27.0",
    "remixicon": "^4.5.0",
    "scrollreveal": "^4.0.9",
    "swiper": "^11.2.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.11.1",
    "@types/node": "^22.13.10",
    "@types/react": "^18.3.10",
    "@types/react-dom": "^18.3.0",
    "@types/react-helmet": "^6.1.11",
    "@types/react-image-gallery": "^1.2.4",
    "@types/scrollreveal": "^0.0.11",
    "@types/swiper": "^5.4.3",
    "@vitejs/plugin-react": "^4.3.2",
    "eslint": "^9.11.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.12",
    "globals": "^15.9.0",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.7.0",
    "vite": "^5.4.8"
  }
}
