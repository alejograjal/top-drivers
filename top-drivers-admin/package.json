{
  "name": "top-drivers-admin",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "top-drivers:generate": "openapi-typescript ./src/api/top-drivers/spec.json -o ./src/api/top-drivers/api.d.ts",
    "top-drivers:update-local": "curl -s http://localhost:5191/swagger/v1/swagger.json -o ./src/api/top-drivers/spec.json && sed -i '' 's/; x-api-version=[0-9]*//' ./src/api/top-drivers/spec.json && npx openapi-typescript ./src/api/top-drivers/spec.json -o ./src/api/top-drivers/api.d.ts --bare"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@hookform/resolvers": "^3.10.0",
    "@mui/icons-material": "^6.4.1",
    "@mui/material": "^6.4.1",
    "@mui/x-data-grid": "^7.24.1",
    "@mui/x-date-pickers-pro": "^7.25.0",
    "@tanstack/react-query": "^5.64.2",
    "@types/lodash": "^4.17.14",
    "dayjs": "^1.11.13",
    "js-cookie": "^3.0.0",
    "jwt-decode": "^4.0.0",
    "lodash": "^4.17.21",
    "openapi-fetch": "^0.13.4",
    "openapi-typescript": "^7.6.0",
    "openapi-typescript-fetch": "^2.1.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-helmet": "^6.1.0",
    "react-hook-form": "^7.54.2",
    "react-number-format": "^5.4.3",
    "react-router-dom": "^7.1.3",
    "yup": "^1.6.1",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.19.0",
    "@types/js-cookie": "^3.0.6",
    "@types/node": "^22.10.10",
    "@types/react": "^19.0.8",
    "@types/react-dom": "^19.0.3",
    "@types/react-helmet": "^6.1.11",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint": "^9.19.0",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.18",
    "globals": "^15.14.0",
    "typescript": "~5.7.3",
    "typescript-eslint": "^8.21.0",
    "vite": "^6.0.11"
